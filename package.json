{
  "name": "auto-gis-with-rust-blog",
  "version": "1.0.0",
  "description": "A blog to document AutoGIS with Rust",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "targets": {
    "default": {
      "distDir": "./docs"
    }
  },
  "scripts": {
    "start": "npm-run-all --parallel dev:*",
